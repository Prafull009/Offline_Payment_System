{
  "expo": {
    "name": "bluepay",
    "slug": "bluepay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0ace2a27-188a-4ffa-befa-34c4b6435624"
      }
    },
    "owner": "prafullb",
    "android": {
      "package": "com.prafullb.bluepay"
    }
  }
}
